<% @cards.each do |card| %>
  <div>
    <strong>User ID</strong><%= card.user_id %>
    <strong>Original</strong>: <%= card.original_text %>
    <strong>Translated</strong>: <%= card.translated_text %>
  </div>
    <div>
      <%= card.review_date %>
   </div>
  <%= link_to 'Delete', card_path(card), method: :delete %>
  <%= link_to 'Edit', edit_card_path(card) %>
  <%= link_to 'Show', card_path(card) %>
  <hr>
<% end %>